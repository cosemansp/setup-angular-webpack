{
  "name": "angular-webpack-startup",
  "version": "1.0.0",
  "description": "Angular-WebPack Startup",
  "scripts": {
    "clean": "rimraf dist",
    "serve": "npm run rm && webpack-dev-server --open --inline --hot",
    "serve:prod": "npm run build:prod && serve ./dist",
    "build": "npm run rm && cross-env NODE_ENV=development webpack",
    "build:prod": "npm run rm && cross-env NODE_ENV=production webpack"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^2.4.5",
    "@angular/compiler": "^2.4.5",
    "@angular/core": "^2.4.5",
    "@angular/platform-browser": "^2.4.5",
    "@angular/platform-browser-dynamic": "^2.4.5",
    "core-js": "^2.4.1",
    "rxjs": "^5.0.3",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "@types/node": "^7.0.5",
    "angular2-template-loader": "^0.6.1",
    "awesome-typescript-loader": "^3.0.3",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-rc",
    "file-loader": "^0.10.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.3.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^5.0.0",
    "serve": "^3.2.7",
    "style-loader": "^0.13.1",
    "tslint": "^4.4.2",
    "tslint-config-standard": "^3.0.0",
    "tslint-eslint-rules": "^3.2.3",
    "typescript": "^2.1.5",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-merge": "^2.6.1"
  }
}
